<%= form_with model: @room do |f| %>
<div class="new_room">
 <div class="room-nav">
  <ul>
   <li class="active">
    <a href="http://127.0.0.1:3000/rooms/new">基本情報</a>
   </li>
  </ul>
 </div>


 <div class="new_room_main">
  <div class="register-box">
   <table>
    <tr>
     <th><%= f.label :name, "ルーム名" %></th>
     <td><%= f.text_field :name %></td>
    </tr>

    <tr>
     <th><%= f.label :introduction, "ルーム紹介" %></th>
     <td><%= f.text_area :introduction %></td>
    </tr>

    <tr>
     <th><%= f.label :price, "料金" %></th>
     <td><%= f.text_field :price %></td>
    </tr>

    <tr>
     <th><%= f.label :address, "住所" %></th>
     <td><%= f.text_field :address %></td>
    </tr>

    <tr>
    <th><%= f.label :image, "ルーム画像" %></th>
    <td><%= f.file_field :image %></td>
   </tr>

   </table>

   <div>
    <ul>
     <li><%= f.submit "登録完了する"%></li>
      </ul>
   </div>
  </div>
 </div>
</div>
 <% end %>